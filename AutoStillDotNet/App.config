<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <connectionStrings>
    <clear />
    <add name="LocalDB" providerName="System.Data.SqlClient" connectionString="server=localhost;database=StillStats;Integrated Security=SSPI;" />
  </connectionStrings>
  <appSettings>
    <add key="InvertPinPolarity" value="True" />
    <add key="fvemptyswitch" value="Type=DigitalInputPin;Pin=1;" />
    <add key="fvcompleteswitch" value="Type=DigitalInputPin;Pin=1;" />
    <add key="stillfluidpump" value="Type=RelayOutputPin;Pin=30;" />
    <add key="stillfillvalve" value="Type=RelayOutputPin;Pin=28;" />
    <add key="stilllowswitch" value="Type=DigitalInputPin;Pin=49;" />
    <add key="stillhighswitch" value="Type=DigitalInputPin;Pin=51;" />
    <add key="stillelement" value="Type=DigitalOutputPin;Pin=53;" />
    <add key="stilldrainvalve" value="Type=RelayOutputPin;Pin=1;" />
    <add key="rvfluidpump" value="Type=RelayOutputPin;Pin=31;" />
    <add key="rvdrainvalve" value="Type=RelayOutputPin;Pin=28;" />
    <add key="rvemptyswitch" value="Type=DigitalInputPin;Pin=45;" />
    <add key="rvfullswitch" value="Type=DigitalInputPin;Pin=47;" />
    <add key="vacuumpump" value="Type=RelayOutputPin;Pin=27;" />
    <add key="fancontroller1" value="Type=AnalogOutputPin;Pin=2" />
    <add key="fancontroller2" value="Type=AnalogOutputPin;Pin=3" />
    <add key="sensorpressure" value="Type=AnalogInputPin;Pin=55;" />
    <add key="sensorcolumntemp" value="Type=AnalogInputPin;Pin=54;" />
    <add key="sensorcoolanttemp1" value="Type=AnalogInputPin;Pin=1;" />
    <add key="sensorcoolanttemp2" value="Type=AnalogInputPin;Pin=1;" />
    <add key="sensorelementamperage" value="Type=AnalogInputPin;Pin=1;" />
    <add key="units" value="Metric" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="RJCP.SerialPortStream" publicKeyToken="5f5e7b70c6a74deb" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.0.0" newVersion="2.2.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>

